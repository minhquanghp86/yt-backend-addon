{
  "name": "YouTube Backend",
  "version": "1.0.4",
  "slug": "yt_youtube_backend",
  "description": "Backend YouTube tự host sử dụng yt-dlp. Cung cấp tìm kiếm, trích xuất video/audio stream và proxy endpoints.",
  "repository": "https://github.com/minhquanghp86/yt-backend-addon",
  "url": "https://github.com/minhquanghp86/yt-backend-addon",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": true,
  "ports": {
    "5000/tcp": 5000
  },
  "map": [
    "config:rw"
  ],
  "options": {
    "API_KEY": "mqsmarthome",
    "PORT": 5000,
    "GO2RTC_URL": "http://localhost:1985"
  },
  "schema": {
    "API_KEY": "str",
    "PORT": "int",
    "GO2RTC_URL": "str"
  },
  "ingress": true,
  "ingress_port": 5000,
  "panel_icon": "mdi:youtube"
}
